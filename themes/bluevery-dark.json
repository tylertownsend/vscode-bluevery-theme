  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#7c7ea2"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#71b7ff"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#bd92ff"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#b3b5cf"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#73ff88"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ff7e87"
      }
    },
    {
