  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#7c7ea2"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#5837a0"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#282b5e"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#006d00"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#b20031"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#b20031"
      }
    },
    {
      "scope": [
