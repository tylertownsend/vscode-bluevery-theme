{
    "name": "vsctb-bluevery-theme",
    "displayName": "Bluevery",
    "publisher": "tylertownsend",
    "description": "A example theme from the vsctb module",
    "version": "0.0.1",
    "repository": {
      "url": "https://github.com/tylertownsend/vscode-bluevery-theme"
    },
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Bluevery Light",
                "uiTheme": "vs",
